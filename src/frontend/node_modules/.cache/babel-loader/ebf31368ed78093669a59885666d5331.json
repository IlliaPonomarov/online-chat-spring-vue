{"ast":null,"code":"import SockJS from \"sockjs-client\";\nimport Stomp from \"webstomp-client\";\nimport AuthService from \"@/services/auth.service\";\nimport ChatService from \"@/services/chat.service\";\nimport axios from \"axios\";\nexport default {\n  name: \"MyChat\",\n\n  data() {\n    return {\n      received_messages: [],\n      your_messages: [],\n      chats: [],\n      friends_messages: [],\n      send_message: null,\n      connected: false,\n      test: null\n    };\n  },\n\n  methods: {\n    logout() {\n      this.$store.dispatch('auth/logout').then(() => {\n        this.$router.push('/auth/login');\n      });\n    },\n\n    send() {\n      this.test = \"Authorization \" + localStorage.getItem(\"X-Authorization\");\n      if (this.send_message !== null) this.your_messages.push(this.send_message);\n      console.log(\"Send message:\" + this.send_message);\n\n      if (this.stompClient && this.stompClient.connected && this.send_message !== null) {\n        const msg = {\n          name: this.send_message\n        };\n        console.log(JSON.stringify(msg));\n        this.stompClient.send(\"/app/chat\", JSON.stringify(msg), {});\n      }\n\n      this.send_message = null;\n    },\n\n    connect() {\n      this.socket = new SockJS(\"http://localhost:8022/gs-chat\");\n      this.stompClient = Stomp.over(this.socket);\n      this.stompClient.connect({\n        \"Authorization\": \"Bearer \" + AuthService.getToken()\n      }, frame => {\n        console.log(\"FUCK   \" + frame);\n        this.connected = true;\n        this.stompClient.subscribe(\"/topic/chat\", tick => {\n          this.received_messages.push(JSON.parse(tick.body).content);\n        });\n      }, error => {\n        console.log(error.body);\n        this.connected = false;\n      });\n    },\n\n    disconnect() {\n      if (this.stompClient) {\n        this.stompClient.disconnect();\n      }\n\n      this.connected = false;\n    },\n\n    tickleConnection() {\n      this.connected ? this.disconnect() : this.connect();\n    }\n\n  },\n\n  mounted() {\n    this.chats.push(axios.get(\"http://localhost:8022/api/chats\", {\n      headers: {\n        Authorization: localStorage.getItem(\"Authorization\")\n      }\n    }).then(response => {\n      return response.data;\n    }));\n    console.log(ChatService.getAllChats().then(response => console.log(response.data)));\n    this.connect();\n  }\n\n};","map":{"version":3,"mappings":"AAoJA,OAAOA,MAAP,MAAmB,eAAnB;AACA,OAAOC,KAAP,MAAkB,iBAAlB;AACA,OAAOC,WAAP,MAAwB,yBAAxB;AACA,OAAOC,WAAP,MAAwB,yBAAxB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAGA,eAAe;EACbC,IAAI,EAAE,QADO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,iBAAiB,EAAE,EADd;MAELC,aAAa,EAAE,EAFV;MAGLC,KAAK,EAAE,EAHF;MAKLC,gBAAgB,EAAE,EALb;MAMLC,YAAY,EAAE,IANT;MAOLC,SAAS,EAAE,KAPN;MAQLC,IAAI,EAAE;IARD,CAAP;EAUD,CAdY;;EAebC,OAAO,EAAE;IACPC,MAAM,GAAE;MACN,KAAKC,MAAL,CAAYC,QAAZ,CAAqB,aAArB,EAAoCC,IAApC,CACI,MAAM;QACJ,KAAKC,OAAL,CAAaC,IAAb,CAAkB,aAAlB;MACD,CAHL;IAMD,CARM;;IAUPC,IAAI,GAAG;MACL,KAAKR,IAAL,GAAY,mBAAmBS,YAAY,CAACC,OAAb,CAAqB,iBAArB,CAA/B;MAEA,IAAI,KAAKZ,YAAL,KAAsB,IAA1B,EACE,KAAKH,aAAL,CAAmBY,IAAnB,CAAwB,KAAKT,YAA7B;MAGFa,OAAO,CAACC,GAAR,CAAY,kBAAkB,KAAKd,YAAnC;;MACA,IAAI,KAAKe,WAAL,IAAoB,KAAKA,WAAL,CAAiBd,SAArC,IAAkD,KAAKD,YAAL,KAAsB,IAA5E,EAAkF;QAChF,MAAMgB,GAAE,GAAI;UAAEtB,IAAI,EAAE,KAAKM;QAAb,CAAZ;QACAa,OAAO,CAACC,GAAR,CAAYG,IAAI,CAACC,SAAL,CAAeF,GAAf,CAAZ;QACA,KAAKD,WAAL,CAAiBL,IAAjB,CAAsB,WAAtB,EAAmCO,IAAI,CAACC,SAAL,CAAeF,GAAf,CAAnC,EAAwD,EAAxD;MAEF;;MACA,KAAKhB,YAAL,GAAoB,IAApB;IACD,CAzBM;;IA0BPmB,OAAO,GAAG;MAER,KAAKC,MAAL,GAAc,IAAI/B,MAAJ,CAAW,+BAAX,CAAd;MACA,KAAK0B,WAAL,GAAmBzB,KAAK,CAAC+B,IAAN,CAAW,KAAKD,MAAhB,CAAnB;MAEA,KAAKL,WAAL,CAAiBI,OAAjB,CACI;QAAC,iBAAiB,YAAY5B,WAAW,CAAC+B,QAAZ;MAA9B,CADJ,EAEIC,KAAI,IAAK;QAEPV,OAAO,CAACC,GAAR,CAAY,YAAYS,KAAxB;QACA,KAAKtB,SAAL,GAAiB,IAAjB;QACA,KAAKc,WAAL,CAAiBS,SAAjB,CAA2B,aAA3B,EAA0CC,IAAG,IAAK;UAChD,KAAK7B,iBAAL,CAAuBa,IAAvB,CAA4BQ,IAAI,CAACS,KAAL,CAAWD,IAAI,CAACE,IAAhB,EAAsBC,OAAlD;QACD,CAFD;MAGD,CATL,EAUIC,KAAI,IAAK;QACP